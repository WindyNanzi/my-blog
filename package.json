{
  "private": true,
  "name": "dumi-app",
  "type": "module",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "deploy": "node scripts/deploy.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "chalk": "^5.0.0",
    "dumi": "^1.0.13",
    "execa": "^6.1.0",
    "lint-staged": "^10.0.7",
    "node-ssh": "^12.0.3",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}
